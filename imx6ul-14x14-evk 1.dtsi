&ecspi1 {
	compatible = "fsl,imx6ul-ecspi";
	fsl,spi-num-chipselects = <4>;
	cs-gpios = <&gpio4 21 GPIO_ACTIVE_LOW>, <&gpio4 22 GPIO_ACTIVE_LOW>, <&gpio4 23 GPIO_ACTIVE_LOW>, <&gpio4 24 GPIO_ACTIVE_LOW>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi1 &pinctrl_ecspi1_cs>;
	status = "okay";

	temp_sensor@0 {
        compatible = "maxim,max31865";
        reg = <0>;
        spi-max-frequency = <400000>;
        spi-cpha;
        maxim,2-wire;
    };
	temp_sensor@1 {
        compatible = "maxim,max31865";
        reg = <1>;
        spi-max-frequency = <400000>;
        spi-cpha;
        maxim,2-wire;
    };
	temp_sensor@2 {
        compatible = "maxim,max31865";
        reg = <2>;
        spi-max-frequency = <400000>;
        spi-cpha;
        maxim,2-wire;
	};
	temp_sensor@3 {
        compatible = "maxim,max31865";
        reg = <3>;
        spi-max-frequency = <400000>;
        spi-cpha;
        maxim,2-wire;
	};
};
