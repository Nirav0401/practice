/ {
	chosen {
		stdout-path = &uart1;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x20000000>;
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		linux,cma {
			compatible = "shared-dma-pool";
			reusable;
			size = <0xa000000>;
			linux,cma-default;
		};
	};

	usdhc2_pwrseq: usdhc2_pwrseq {
		compatible = "mmc-pwrseq-simple";
	};

	wifi_pwrseq: wifi_pwrseq {
      		compatible = "mmc-pwrseq-simple";
      		powerdown-gpios = <&gpio4 17 GPIO_ACTIVE_HIGH>;
      		reset-gpios = <&gpio1 8 GPIO_ACTIVE_HIGH>;
		pinctrl-0 = <&pinctrl_wilc_pwrseq_default>;
		pinctrl-names = "default";
		status = "okay";
    };

	pxp_v4l2 {
		compatible = "fsl,imx6ul-pxp-v4l2", "fsl,imx6sx-pxp-v4l2", "fsl,imx6sl-pxp-v4l2";
		status = "okay";
	};

	reg_usb_ltemodule: regulator@1 {
        	compatible = "regulator-fixed";
            	regulator-name = "ltemodule-pwr";
            	regulator-min-microvolt = <3800000>;
           	regulator-max-microvolt = <3800000>;
            	gpios = <&gpio5 5 GPIO_ACTIVE_HIGH>;
            	enable-active-high;
            	regulator-boot-on;
            	regulator-always-on;
	};

	reg_gpio_wifi: regulator@2 {
            	compatible = "regulator-fixed";
            	regulator-name = "wifi-pwr";
           	regulator-min-microvolt = <3300000>;
            	regulator-max-microvolt = <3300000>;
 	};

	reg_sd1_vmmc: regulator-sd1-vmmc {
		compatible = "regulator-fixed";
		regulator-name = "VSD_3V3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpio1 9 GPIO_ACTIVE_HIGH>;
		off-on-delay-us = <20000>;
		enable-active-high;
	};

	reg_adc1_vref_3v3: regulator@5 {
		compatible = "regulator-fixed";
		regulator-name = "vref-3v3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
	};

